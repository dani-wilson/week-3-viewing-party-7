<% if current_user %>
    <%= link_to "Log Out", logout_path, method: :delete %><br>
    <%= link_to "View My Dashboard", user_path(current_user) %>
<% else %>
    <%= link_to "Log In", login_path, method: :get %><br>
    <%= link_to "View My Dashboard", user_path(0) %>
    <%= button_to "Create New User", register_path, method: :get %>
<% end %>

<section class='existing-users'>
<% if current_user %>
    <p>Existing Users: </p>
    <% @users.each do |user| %>
        <li><%= user.email %></li>
    <% end %>
<% end %>
</section>